set colorcolumn=81
syntax enable
set tabstop=4
set softtabstop=4
set expandtab
set number
set showcmd
set cursorline
set showmatch
set hlsearch
nnoremap ; :
nnoremap K 10k
nnoremap J 10j
nnoremap L 10l
nnoremap H 10h
map <C-j> <C-W>j
map <C-k> <C-W>k
map <C-h> <C-W>h
map <C-l> <C-W>l
nnoremap B ^
nnoremap E $
